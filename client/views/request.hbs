<div class="panel panel-primary">
  <div class="panel-heading">
    <h3 class="panel-title">File Request</h3>
  </div>
  <div class="panel-body">
    <form class="form-horizontal" id="search-form" enctype="multipart/form-data" method="post" action="javascript:">
      <div class="form-group">
        <label for="fid" class="col-sm-2 control-label">File ID</label>
        <div class="col-sm-10">
          <input type="text" class="form-control" name="fid" id="fid" placeholder="file ID">
        </div>
      </div>
      <div class="form-group">
        <label for="name" class="col-sm-2 control-label">Title</label>
        <div class="col-sm-10">
          <input type="text" class="form-control" name="title" id="title" placeholder="file title">
        </div>
      </div>
      <div class="form-group">
        <label for="owner" class="col-sm-2 control-label">Owner</label>
        <div class="col-sm-10">
          <input type="text" class="form-control" name="owner" id="owner" placeholder="file's owner">
        </div>
      </div>
       <div class="form-group">
        <label for="keyword" class="col-sm-2 control-label">Keywords</label>
        <div class="col-sm-10">
          <input type="text" class="form-control" name="keyword" id="keyword" placeholder="file's keywords">
        </div>
      </div>
      <div class="form-group">
        <div class="col-sm-offset-10 col-sm-10">
          <button id="search" class="btn btn-primary">Search</button>
        </div>
      </div>
    </form>
  </div>
</div>

<div id="searchItem" style="display:none;">
  <div class="panel panel-primary">
    <div class="panel-heading">
      <h3 class="panel-title">Found Item</h3>
    </div>
    <div class="panel-body">
      <strong>fid</strong>: <span class="fid">XXX</span><br />
      <strong>Name</strong>: <span class="title">XXX</span><br />
      <strong>Keyword</strong>: <span class="keyword">XXX</span><br />
      <strong>Summary</strong>: <span class="summary">XXX</span><br />
      <strong>Owner</strong>: <span class="owner">XXX</span><br />
      <strong>hash</strong>: <span class="hash">XXX</span><br />
      <strong>Price</strong>: <span class="price">XXX</span><br />
      <strong>Handler</strong>: <span class="handler">XXX</span><br />
      <button class="btn btn-primary request" type="button">Request</button>
    </div>
  </div>
</div>

<div id="searchList">
  <!-- load my files (fileTemplate) here -->
</div>

<script src="/javascripts/request.js"></script>